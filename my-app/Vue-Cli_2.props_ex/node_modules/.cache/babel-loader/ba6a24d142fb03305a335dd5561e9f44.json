ed() {\n    console.log(\"유저 컴포넌트\");\n    this.createdAt = new Date();\n  },\n  methods: {\n    parents: function parents(user) {\n      // user = {name,address , phone , hasDog}\n      this.name = user.name;\n      this.address = user.address;\n      this.phone = user.phone;\n      this.hasDog = user.hasDog;\n      console.log(\"부모가 받았어!\");\n    },\n    getDateAndTime: function getDateAndTime(date) {\n      if (date != null) {\n        var hour = date.getHours();\n        var minutes = date.getMinutes();\n        var fullDate = \"\".concat(date.getFullYear(), \"/\").concat(date.getMonth() + 1, \"/\").concat(date.getDate());\n        return \" \".concat(hour, \":\").concat(minutes, \" \").concat(fullDate);\n      } else {\n        return null;\n      }\n    }\n  },\n  //mixin :같은기능을 여러곳에서 수행할수있게해줌 ,mixin은 array 형태로\n  mixins: [dateFormat]\n};",{"version":3,"sources":["User.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiCA;AACA;AACA;AACA,OAAA,UAAA,MAAA,kBAAA;AACA,OAAA,QAAA,MAAA,gBAAA;AACA,SAAA,UAAA,QAAA,sBAAA;AACA,eAAA;AACA,EAAA,UAAA,EAAA;AACA,IAAA,UAAA,EAAA,UADA;AAEA,IAAA,QAAA,EAAA;AAFA,GADA;AAKA,EAAA,IALA,kBAKA;AACA,WAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,OAAA,EAAA,OAFA;AAGA,MAAA,KAAA,EAAA,WAHA;AAIA,MAAA,MAAA,EAAA,IAJA;AAKA,MAAA,SAAA,EAAA;AALA,KAAA;AAOA,GAbA;AAcA,EAAA,QAAA,EAAA;AACA,IAAA,YADA,0BACA;AACA;AACA,aAAA,KAAA,SAAA,GAAA,QAAA;AACA,KAJA;AAKA,IAAA,IALA,kBAKA;AACA,UAAA,CAAA,GAAA,KAAA;AACA,MAAA,CAAA,GAAA,KAAA;AACA,aAAA,CAAA;AACA;AATA,GAdA;AAyBA,EAAA,OAzBA,qBAyBA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,SAAA;AACA,SAAA,SAAA,GAAA,IAAA,IAAA,EAAA;AACA,GA5BA;AA6BA,EAAA,OAAA,EAAA;AACA,IAAA,OADA,mBACA,IADA,EACA;AACA;AACA,WAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,WAAA,OAAA,GAAA,IAAA,CAAA,OAAA;AACA,WAAA,KAAA,GAAA,IAAA,CAAA,KAAA;AACA,WAAA,MAAA,GAAA,IAAA,CAAA,MAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,UAAA;AACA,KARA;AASA,IAAA,cATA,0BASA,IATA,EASA;AACA,UAAA,IAAA,IAAA,IAAA,EAAA;AACA,YAAA,IAAA,GAAA,IAAA,CAAA,QAAA,EAAA;AACA,YAAA,OAAA,GAAA,IAAA,CAAA,UAAA,EAAA;AACA,YAAA,QAAA,aAAA,IAAA,C